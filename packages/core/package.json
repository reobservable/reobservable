  "license": "ISC",
  "dependencies": {
    "invariant": "^2.2.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "lodash.mergewith": "^4.6.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.2"
  },
  "sideEffects": false,
  "devDependencies": {
    "axios": "^0.18.0"
  }
}
